{
  "name": "Vocab Bot V2",
  "manifest_version": 3,
  "version": "1.1.3",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications"
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.vocabulary.com/lists/*/bee*"
      ],
      "js": [
        "index.js"
      ]
    },
    {
      "matches": [
        "https://www.vocabulary.com/lists/*/bee*"
      ],
      "run_at": "document_start",
      "js": [
        "score.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "service_worker": "background.js"
  },
  "content_security_policy": {},
  "host_permissions": [
    "https://*.vocabulary.com/*"
  ]
}