"permissions":["storage","unlimitedStorage"],
"options_page": "options.html",
"options_ui": {
  "page": "options.html",
  "open_in_tab": false
},
 "content_scripts": [

   {

     "matches": ["https://www.vocabulary.com/lists/*/bee"],

     

